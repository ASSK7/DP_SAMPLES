{
	"_version": "1.7.0",
	"sap.app": {
		"id": "com.dpSalesRepo",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_belize"]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "com.dpSalesRepo.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.dpSalesRepo.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}],
			"js": [{
				"uri": "libs/jspdf.debug.js"
			}, {
				"uri": "libs/jspdf.plugin.autotable.js"
			}, {
				"uri": "libs/filesaver.js"
			}]
		},
		"routing": {
			"config": {
				"viewPath": "com.dpSalesRepo.view",
				"controlId": "idApp"
			},
			"targets": {
				"display": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "display",
					"controlAggregation": "pages",
					"controlId": "idApp",
					"viewId": "view1"
				}
			},
			"routes": [{
				"name": "display",
				"pattern": "",
				"titleTarget": "",
				"greedy": false,
				"target": ["display"]
			}]
		}
	}
}